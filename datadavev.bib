@article{dewitCurrentBrowningSurface2016,
  title = {Current {{Browning}} of {{Surface Waters Will Be Further Promoted}} by {{Wetter Climate}}},
  author = {{de Wit}, Heleen A. and Valinia, Salar and Weyhenmeyer, Gesa A. and Futter, Martyn N. and Kortelainen, Pirkko and Austnes, Kari and Hessen, Dag O. and R{\"a}ike, Antti and Laudon, Hjalmar and Vuorenmaa, Jussi},
