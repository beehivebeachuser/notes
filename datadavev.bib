@article{cousinsOutsideSafeOperating2022,
  title = {Outside the {{Safe Operating Space}} of a {{New Planetary Boundary}} for {{Per-}} and {{Polyfluoroalkyl Substances}} ({{PFAS}})},
  author = {Cousins, Ian T. and Johansson, Jana H. and Salter, Matthew E. and Sha, Bo and Scheringer, Martin},
  year = {2022},
  month = aug,
  journal = {Environmental Science \& Technology},
